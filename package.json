{
  "private": true,
  "scripts": {
    "start-fable": "cd src && dotnet fable webpack-dev-server --port free",
    "build-fable": "cd src && dotnet fable webpack --port free -p",
    "sass": "node-sass ./sass/main.sass ./public/build/main.css",
    "start": "concurrently -k -p \" \" \"npm run start-fable\" \"npm run sass && npm run sass -- -w\"",
    "build": "concurrently -p \" \" \"npm run build-fable\" \"npm run sass\""
  },
